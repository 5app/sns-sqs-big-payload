		"prepublishOnly": "node --run build"
	},
	"dependencies": {
		"@5app/prettier-config": "^1.0.4",
		"@aws-sdk/client-s3": "^3.758.0",
		"@aws-sdk/client-sns": "^3.758.0",
		"@aws-sdk/client-sqs": "^3.758.0",
		"@aws-sdk/lib-storage": "^3.758.0",
		"@smithy/smithy-client": "^4.1.6"
	},
	"devDependencies": {
		"@5app/semantic-release-config": "^1.1.0",
		"@tsconfig/node22": "^22.0.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.13.9",
		"jest": "^29.7.0",
		"prettier": "^3.5.3",
		"ts-jest": "^29.2.6",
		"typescript": "^5.8.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/5app/sns-sqs-big-payload"
	},
	"bugs": {
		"url": "https://github.com/5app/sns-sqs-big-payload/issues"
	},
	"homepage": "https://github.com/5app/sns-sqs-big-payload",
	"prepublish": "tsc",
	"jest": {
