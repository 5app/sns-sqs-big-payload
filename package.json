			"^.+\\.(ts)$": "ts-jest"
		},
		"testEnvironment": "node",
		"maxConcurrency": 1
	},
	"prettier": "@5app/prettier-config",
	"release": {
		"extends": "@5app/semantic-release-config"
	},
	"keywords": [
		"sqs",
		"sns",
		"queue",
		"consumer",
		"large",
		"big",
		"payload"
