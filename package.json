{
    "name": "@5app/sns-sqs-big-payload",
    "version": "0.1.4",
    "license": "Apache-2.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest",
        "build": "node --run clean && tsc",
        "clean": "rm -rf ./dist/*",
        "prepublishOnly": "node --run build"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.758.0",
        "@aws-sdk/client-sns": "^3.758.0",
        "@aws-sdk/client-sqs": "^3.758.0",
        "@aws-sdk/lib-storage": "^3.758.0",
        "@smithy/smithy-client": "^4.1.6",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@tsconfig/node22": "^22.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.9",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.6",
        "typescript": "^5.8.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/5app/sns-sqs-big-payload"
    },
    "bugs": {
        "url": "https://github.com/5app/sns-sqs-big-payload/issues"
    },
    "homepage": "https://github.com/5app/sns-sqs-big-payload",
    "prepublish": "tsc",
    "jest": {
        "preset": "ts-jest",
        "testMatch": [
            "**/tests/**/*.spec.+(ts|tsx|js)"
        ],
        "globals": {
            "ts-jest": {
                "diagnostics": false
            }
        },
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transform": {
            "^.+\\.(ts)$": "ts-jest"
        },
        "testEnvironment": "node",
        "maxConcurrency": 1
